{
    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "ruff.format.args": [
        "--line-length=149"
    ],
    "python.formatting.provider": "none",
    "editor.formatOnSave": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
}